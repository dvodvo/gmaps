<script src="//maps.google.com/maps/api/js?v=3.13&amp;sensor=false&amp;libraries=geometry" type="text/javascript"></script>
<script src='//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js' type='text/javascript'></script>
<script src='//google-maps-utility-library-v3.googlecode.com/svn/trunk/richmarker/src/richmarker-compiled.js' type='text/javascript'></script>
<script src='//google-maps-utility-library-v3.googlecode.com/svn/tags/infobox/1.1.9/src/infobox_packed.js' type='text/javascript'></script>

<h1>Listing strutturas</h1>

<table>
  <tr>
    <th>Latitude</th>
    <th>Longitude</th>
    <th>Nome</th>
    <th>Servizi</th>
    <th>Telefono</th>
    <th>Web</th>
    <th>Address</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @strutturas.each do |struttura| %>
  <tr>
    <td><%= struttura.latitude %></td>
    <td><%= struttura.longitude %></td>
    <td><%= struttura.nome %></td>
    <td><%= struttura.servizi %></td>
    <td><%= struttura.telefono %></td>
    <td><%= struttura.web %></td>
    <td><%= struttura.address %></td>
    <td><%= link_to 'Show', struttura %></td>
    <td><%= link_to 'Edit', edit_struttura_path(struttura) %></td>
    <td><%= link_to 'Destroy', struttura, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Struttura', new_struttura_path %>

<div style='width: 800px;'>
  <div id="map" style='width: 800px; height: 400px;'></div>
</div>

<script type="text/javascript">
buildMap(<%=raw @hash.to_json %>)
</script>